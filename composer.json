{
  "name": "drenso/symfony-oidc-bundle",
  "description": "OpenID connect bundle for Symfony",
  "keywords": [
    "symfony",
    "oidc",
    "openid connect"
  ],
  "type": "symfony-bundle",
  "homepage": "https://gitlab.drenso.nl/intern/symfony-oidc",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Bob van de Vijver",
      "email": "bob@drenso.nl"
    },
    {
      "name": "Tobias Feijten",
      "email": "tobias@drenso.nl"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-curl": "*",
    "ext-hash": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "symfony/http-foundation": "^3.4.35 || ^4.2.12 || ^5.0.7",
    "symfony/routing": "^3.4 || ^4.0 || ^5.0",
    "psr/log": "^1.1",
    "phpseclib/phpseclib": "^2.0.31 || ^3.0.7",
    "symfony/dependency-injection": "^3.4.26 || ^4.1.12 || ^5.0",
    "symfony/security-bundle": "^3.4 || ^4.0 || ^5.0",
    "symfony/security-core": "^3.4 || ^4.0 || ^5.0",
    "symfony/security-http": "^3.4.26 || ^4.1.12 || ^5.0.7"
  },
  "require-dev": {
    "drenso/phan-extensions": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Drenso\\OidcBundle\\": "src"
    }
  }
}
