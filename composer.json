{
  "name": "drenso/symfony-oidc-bundle",
  "description": "OpenID connect bundle for Symfony",
  "keywords": [
    "symfony",
    "oidc",
    "openid connect"
  ],
  "type": "symfony-bundle",
  "homepage": "https://gitlab.drenso.nl/intern/symfony-oidc",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Bob van de Vijver",
      "email": "bob@drenso.nl"
    },
    {
      "name": "Tobias Feijten",
      "email": "tobias@drenso.nl"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "symfony/http-foundation": "^3.4 || ^4.2.12",
    "symfony/routing": "^3.4 || ^4.0",
    "phpseclib/phpseclib": "^2.0",
    "symfony/security": "^3.4 || ^4.0",
    "symfony/dependency-injection": "^3.4 || ^4.1.12",
    "symfony/security-bundle": "^3.4 || ^4.0"
  },
  "require-dev": {
    "drenso/phan-extensions": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Drenso\\OidcBundle\\": "src"
    }
  }
